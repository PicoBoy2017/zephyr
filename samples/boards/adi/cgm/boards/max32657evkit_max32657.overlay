/ {
	aliases {
		pb = &pb1;
		led = &led1;
	};
};

&flash0 {
	partitions {
		slot0_partition: partition@10000 {
			label = "image-0";
			reg = <0x10000 DT_SIZE_K(356)>;
		};
	};
};

&i3c0 {
	adxl367: adxl367@530000000000000000 {
		compatible = "adi,adxl367";
		reg = <0x53 0x00 0x00>;
		status = "okay";
	};
};
